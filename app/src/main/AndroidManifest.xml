<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phinma_upangstudentsupportmodule">

    <meta-data
        android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PHINMAUPangStudentSupportModule">
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".AssessmentScreenActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForgotPassword" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".Login" />
        <activity android:name=".AssessmentResults" />
        <activity android:name=".AssessmentActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>